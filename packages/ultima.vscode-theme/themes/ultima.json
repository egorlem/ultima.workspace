{
  "name": "Ultima",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Constants (Group 1)",
      "scope": [
        "keyword",
        "storage",
        "keyword.operator",
        "keyword.operation",
        "keyword.control",
        "keyword.other",
        "punctuation.separator",
        "punctuation.definition.template-expression",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.keyword",
        "punctuation.definition.block.tag.jsdoc",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "variable.language.this",
        "meta.parameters",
        "meta.tag",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#5BA2D9"
      }
    },
    {
      "name": "Markdown (Group 1)",
      "scope": [
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#5BA2D9"
      }
    },
    {
      "name": "Graphql (Group 1)",
      "scope": [
        "meta.brace.round.graphql"
      ],
      "settings": {
        "foreground": "#5BA2D9"
      }
    },
    {
      "name": "Variables, Object (Group 2)",
      "scope": [
        "variable",
        "text.html.derivative",
        "meta.field",
        "meta.jsx.children",
        "meta.object-literal.key",
        "support.type.property-name.json",
        "entity.other.attribute-name",
        "meta.paragraph.markdown"
      ],
      "settings": {
        "foreground": "#DDE3DC"
      }
    },
    {
      "name": "Markdown (Group 2)",
      "scope": [
        "meta.paragraph.markdown",
        "text.html.markdown"
      ],
      "settings": {
        "foreground": "#DDE3DC"
      }
    },
    {
      "name": "CSS Class and Support (Group 3)",
      "scope": [
        "support.function.misc",
        "support.type.property-name",
        "entity.name.function",
        "meta.method",
        "meta.function",
        "meta.function-call",
        "meta.brace",
        "variable.other.jsdoc",
        "constant.other.reference",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#59D9D0"
      }
    },
    {
      "name": "Markdown (Group 3)",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#59D9D0"
      }
    },
    {
      "name": "Class (Group 4)",
      "scope": [
        "support.variable.property",
        "support.class",
        "support.type",
        "meta.class",
        "meta.selector entity.other.attribute-name",
        "entity.other.attribute-name.class",
        "support.class.component",
        "support.class.builtin",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#7CD996"
      }
    },
    {
      "name": "TypeScript (Group 4)",
      "scope": [
        "keyword.operator.type.annotation",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#7CD996"
      }
    },
    {
      "name": "Grapql (Group 4)",
      "scope": [
        "entity.name.fragment.graphql",
        "variable.fragment.graphql"
      ],
      "settings": {
        "foreground": "#7CD996"
      }
    },
    {
      "name": "Number, (Group 5)",
      "scope": [
        "constant.numeric",
        "constant.character.entity",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined",
        "constant.other.color",
        "constant.character.escape",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#8581DA"
      }
    },
    {
      "name": "TypeScript, (Group 5)",
      "scope": [
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#8581DA"
      }
    },
    {
      "name": "CSS SASS, (Group 5)",
      "scope": [
        "constant.numeric.css keyword.other.unit"
      ],
      "settings": {
        "foreground": "#8581DA"
      }
    },
    {
      "name": "Regular Expressions definition (Group 6)",
      "scope": [
      ],
      "settings": {
        "foreground": "#B7D074"
      }
    },
    {
      "name": "Markdown (Group 6)",
      "scope": [
        "fenced_code.block.language.markdown"
      ],
      "settings": {
        "foreground": "#B7D074"
      }
    },
    {
      "name": "punctuation dim, Comments (Group 7)",
      "scope": [
        "comment",
        "punctuation",
        "punctuation.separator.array",
        "meta.brace.square",
        "punctuation.separator.comma",
        "punctuation.separator.parameter",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#616360"
      }
    },
    {
      "name": "SASS, SASS, CSS (Group 7)",
      "scope": [
        "meta.property-list"
      ],
      "settings": {
        "foreground": "#616360"
      }
    },
    {
      "name": "Punctuation (Group 8)",
      "scope": [
      ],
      "settings": {
        "foreground": "#616360"
      }
    },
    {
      "name": "String (Group 9)",
      "scope": [
        "string",
        "support.constant.property-value",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#B7D074"
      }
    },
    {
      "name": "Escape Characters (Group Special)",
      "scope": [
        "string.quoted.strict"
      ],
      "settings": {
        "foreground": "#5fffaf"
      }
    },
    {
      "name": "Escape Characters (Group Special)",
      "scope": [
        ""
      ],
      "settings": {
        "foreground": "#5fffaf"
      }
    },
    {
      "name": "Invalid (Group Special)",
      "scope": [
        "keyword.other.debugger",
        "keyword.todo",
        "keyword.note",
        "keyword.fixit",
        "keyword.bug",
        "invalid",
        "string.quoted.pragma",
        "comment.pragma"
      ],
      "settings": {
        "foreground": "#E85181"
      }
    },
    {
      "name": "Common text (Group Special)",
      "scope": [ "source" ],
      "settings": {
        "foreground": "#cccccc"
      }
    }
  ],
  "colors": {
    "editorWarning.foreground": "#B7D074",
    "editorError.foreground": "#E85181",
    "inputValidation.warningBorder": "#B7D074",
    "inputValidation.errorBorder": "#E85181",
    "list.warningForeground": "#B7D074",
    "list.errorForeground": "#E85181",
    "notificationLink.foreground": "#5BA2D9",
    "textLink.activeForeground": "#5BA2D9",
    "textLink.foreground": "#5BA2D9",
    "list.activeSelectionBackground": "#005f5f",
    "gitDecoration.untrackedResourceForeground": "#5BA2D9",
    "gitDecoration.modifiedResourceForeground": "#59D9D0",
